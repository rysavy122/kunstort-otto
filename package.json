{
  "name": "spa-angular-typescript-hello-world",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "env": "ts-node set-env.ts",
    "start": "NODE_ENV=development npm run env && ng serve",
    "build:development": "NODE_ENV=development npm run env && ng build --configuration development",
    "build:staging": "NODE_ENV=staging npm run env && ng build --configuration production && npm run copy:webconfig",
    "build:production": "NODE_ENV=production npm run env && ng build --configuration production && npm run copy:webconfig",
    "copy:webconfig": "copyfiles -u 1 src/web.config dist/spa_angular_typescript_hello-world/",
    "watch": "ng build --watch --configuration development"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "15.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@auth0/auth0-angular": "^2.0.0",
    "@ckeditor/ckeditor5-angular": "^7.0.1",
    "@ckeditor/ckeditor5-build-classic": "^40.2.0",
    "@flatten-js/core": "^1.4.7",
    "@fortawesome/angular-fontawesome": "^0.12.1",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@tinymce/tinymce-angular": "^7.0.0",
    "axios": "^0.27.2",
    "bezier-js": "^6.1.4",
    "gsap": "^3.12.4",
    "jwt-decode": "^4.0.0",
    "ngx-toastr": "^16.2.0",
    "paper": "^0.12.17",
    "rxjs": "~7.5.0",
    "snapsvg": "^0.5.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular/cli": "~15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@types/gsap": "^3.0.0",
    "@types/jasmine": "~4.0.0",
    "@types/snapsvg": "^0.5.8",
    "@types/tinymce": "^4.6.9",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.1",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.4"
  }
}
